{
  "name": "slacklinemtl",
  "version": "1.0.0",
  "private": true,
  "description": "www.slacklinemtl.com",
  "author": "Felix Paradis",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "gatsby": "^3.13.1",
    "gatsby-plugin-google-analytics": "^3.14.0",
    "gatsby-plugin-image": "^1.14.0",
    "gatsby-plugin-mdx": "^2.14.0",
    "gatsby-plugin-no-sourcemaps": "^3.14.0",
    "gatsby-plugin-preact": "^5.14.0",
    "gatsby-plugin-react-helmet": "^4.14.0",
    "gatsby-plugin-sharp": "^3.14.0",
    "gatsby-plugin-sitemap": "^4.10.0",
    "gatsby-source-filesystem": "^3.14.0",
    "gatsby-source-shopify": "^5.6.0",
    "gatsby-transformer-sharp": "^3.14.0",
    "preact": "^10.5.14",
    "preact-render-to-string": "^5.1.19",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-wesbos": "^2.0.1",
    "eslint-plugin-html": "^6.1.2",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.3",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gatsby-plugin-postcss": "^4.14.0"
  },
  "eslintConfig": {
    "extends": [
      "wesbos"
    ],
    "rules": {
      "no-use-before-define": [
        "error",
        {
          "functions": false
        }
      ],
      "react/prop-types": 0,
      "semi": [
        1,
        "never"
      ],
      "jsx-a11y/label-has-associated-control": [
        "error",
        {
          "required": {
            "some": [
              "nesting",
              "id"
            ]
          }
        }
      ],
      "jsx-a11y/label-has-for": [
        "error",
        {
          "required": {
            "some": [
              "nesting",
              "id"
            ]
          }
        }
      ],
      "import/no-extraneous-dependencies": [
        "error",
        {
          "devDependencies": true
        }
      ]
    }
  },
  "prettier": {
    "semi": false
  },
  "babel": {
    "presets": [
      [
        "babel-preset-gatsby",
        {
          "reactRuntime": "automatic"
        }
      ]
    ]
  }
}
